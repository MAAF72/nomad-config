
    # START: CONFIG_APACHE_||APPLICATION_ID|| #

    alias /||APPLICATION_NAME|| ||APPLICATION_MAIN_DIR||

    <Directory ||APPLICATION_MAIN_DIR||>
        # AllowOverride All
        <IfModule mod_rewrite.c>
            RewriteEngine On
            RewriteBase /||APPLICATION_NAME||
            RewriteRule ^index\.php$ - [L]
            RewriteCond %{REQUEST_FILENAME} !-f
            RewriteCond %{REQUEST_FILENAME} !-d
            RewriteRule . index.php [L]
        </IfModule>
        <FilesMatch \.php$>
            # START: CONFIG_APACHE_PHP_||APPLICATION_ID|| #
            SetHandler "proxy:unix:/var/run/php/php7.4-fpm.sock|fcgi://localhost" 
            # END: CONFIG_APACHE_PHP_||APPLICATION_ID|| #
        </FilesMatch>
    </Directory>

    # END: CONFIG_APACHE_||APPLICATION_ID|| #
